{
    "name": "Custom TV Controller",
    "url": "https://github.com/quentinchap/tv-custom-api/releases/tag/0.1.3",
    "version": "0.1.3",
    "slug": "api_channel",
    "description": "Allow to add possibilities for channel selection. Exemple: TF1 = 1 = tf1 = tf 1, etc.",
    "webui": "http://[HOST]:[PORT:8245]",
    "startup": "application",
    "boot": "auto",
    "hassio_api": true,
    "homeassistant_api": true,
    "host_network": false,
    "ports": {
        "5050/tcp": 5050
    },
    "map": [
        "config:rw",
        "share:rw",
        "ssl:rw"
    ],
    "options": {
        "log_level": "info",
        "commtype": "websockets"
    },
    "schema": {
        "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
        "commtype": "match(^(websockets|sse)$)"
    },
    "environment": {
        "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
    },
    "image": "quentinchap/tv-custom-api-{arch}"
}